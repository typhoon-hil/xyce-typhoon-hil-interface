                            for idx in range(len(signs)):

                                if idx == 0:
                                    sum_input_name = "in"
                                else:
                                    sum_input_name = "in"+str(idx)

                                new_input = mdl.create_port(
                                name="In"+str(idx+1),
                                label = signs[idx],
                                parent=comp_handle,
                                rotation='up',
                                kind='sp',
