                                                new_pos = (key, new_term_positions[key].index(term_name) + 1)
                                                mdl.set_port_properties(ph, terminal_position = new_pos)
                                                pin_pos_dict.update({term_name:(new_pos[0],str(new_pos[1]))})
                    
                            for term in terminals:
