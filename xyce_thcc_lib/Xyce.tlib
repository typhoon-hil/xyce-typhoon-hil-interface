                                # 3-pin MESFETs
                                num_pins = 3
                                # Find subcircuit-based models
                                sub_based = re.compile(r"^\.SUBCKT\s+(\S+)\s+([ A-z0-9_]+)\s+(?:PARAMS)?",
                                                        flags = re.I)
                                # Find individual models
                                nmf_models = re.compile(r"^\.MODEL ([\S]*)\s+NMF", flags = re.I)
